.leftSidebar
	+mainCardTemplate("روزنامه آفرینش","pdfSlider")
		.carousel.slide(data-ride='carousel')
			.carousel-inner
				.carousel-item.active
					.slide-content.flex
						a.pdf(href="")
							img(src='../assets/img/pdfbul.jpg', alt='img1')
						a.image(href="")
							img(src='../assets/img/pdfimg.jpg', alt='img1')
				each n in[1,2,3,4,5,6]
					.carousel-item
						.slide-content.flex
							a.pdf(href="")
								img(src='../assets/img/pdfbul.jpg', alt='img1')
							a.image(href="")
								img(src='../assets/img/pdfimg.jpg', alt='img1')
			ul.carousel-indicators
				each n in [1,2,3,4,5,6,7]
					li(data-target='', data-slide-to='0')=n
